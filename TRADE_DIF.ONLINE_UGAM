create or replace view KSFPA.ONLINE_UGAM_PVT.TRADE_DIF(
	YEAR,
	PERIOD,
	WEEK,
	FY_PW,
	SUM_QTY_PCKD_TODAY,
	SUM_ORG_QTY_TODAY
) as
SELECT DISTINCT YEAR,
       PERIOD,
       WEEK,
       FY_PW,
       SUM(SUM_QTY_PCKD_TODAY) AS SUM_QTY_PCKD_TODAY,
       SUM(SUM_ORG_QTY_TODAY) AS SUM_ORG_QTY_TODAY	
FROM "KSFPA"."ONLINE_UGAM_PVT"."WKLY_DIF_PERCENTAGE_REALTIME"
GROUP BY 1,2,3,4;